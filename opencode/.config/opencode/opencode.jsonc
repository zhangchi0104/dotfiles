{
  "plugin": ["oh-my-opencode@latest", "opencode-antigravity-auth@1.3.2"],
  "$schema": "https://opencode.ai/config.json",
  "theme": "opencode",
  "model": "google-vertex-anthropic/claude-opus-4-5@20251101",
  "small_model": "google-vertex-anthropic/claude-haiku-4-5@20251001",
  "autoupdate": true,
  "mcp": {
    "serena": {
      "enabled": true,
      "type": "local",
      "command": [
        "uvx",
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
      ],
    },
    "context7": {
      "type": "local",
      "command": ["npx", "-y", "@upstash/context7-mcp"],
      "enabled": true,
    },
  },
  "permission": {
    "edit": {
      "*": "ask",
    },
  },
  "provider": {
    "google": {
      "name": "Google",
      "models": {
        "antigravity-gemini-3-pro": {
          "name": "Gemini 3 Pro (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65535,
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "low": {
              "thinkingLevel": "low",
            },
            "high": {
              "thinkingLevel": "high",
            },
          },
        },
        "antigravity-gemini-3-flash": {
          "name": "Gemini 3 Flash (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65536,
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "minimal": {
              "thinkingLevel": "minimal",
            },
            "low": {
              "thinkingLevel": "low",
            },
            "medium": {
              "thinkingLevel": "medium",
            },
            "high": {
              "thinkingLevel": "high",
            },
          },
        },
        "antigravity-claude-sonnet-4-5": {
          "name": "Claude Sonnet 4.5 (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000,
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
        },
        "antigravity-claude-sonnet-4-5-thinking": {
          "name": "Claude Sonnet 4.5 Thinking (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000,
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192,
              },
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768,
              },
            },
          },
        },
        "antigravity-claude-opus-4-5-thinking": {
          "name": "Claude Opus 4.5 Thinking (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000,
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"],
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192,
              },
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768,
              },
            },
          },
        },
      },
    },
  },
}
